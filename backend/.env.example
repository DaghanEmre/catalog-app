# Catalog App - Environment Configuration Example
# âœ… Rename this file to .env and adjust values for your environment

# Database Settings
DB_URL=jdbc:postgresql://localhost:5432/catalogdb
DB_USER=catalog
DB_PASSWORD=catalog123

# JWT Security (CRITICAL: Change secret in production)
# Secret must be 32+ characters for HS256
JWT_SECRET=CHANGE_ME_TO_A_VERY_LONG_AND_SECURE_RANDOM_STRING_32_CHARS
JWT_ISSUER=catalog-app
JWT_EXPIRATION_MINUTES=60

# Application Control
SPRING_PROFILES_ACTIVE=dev
DATA_SEEDING_ENABLED=true
